@{
    var React = (CoursesModel)ViewData["React"];

}
<div class="container">

    <div class="row ">
        <h1>React :</h1>
    </div>
    <div class="row">
        @foreach (var ReactFile in React.Files)
        {
            <div class="clo">
                <div>
                    <h3>@ReactFile.name</h3>
                    <h3>@ReactFile.date</h3>
                </div>
            </div>
        }
    </div>
</div>